class Main
{
    static  int maxlevel=0;
    static void left_view(Node root,int level)
    {
        if(root==null)
            return;
        if(level>=maxlevel)
        {
            System.out.print(" "+root.data);
            maxlevel++;
        }
        left_view(root.left,level+1);
        left_view(root.right,level+1);
    }
